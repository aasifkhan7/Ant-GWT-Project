<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>com.demo.gwt.server.GreetingServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/onlyant/greet</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet>
    <servlet-name>greetServlet1</servlet-name>
    <servlet-class>com.demo.gwt.server.GreetingServiceImpl1</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>greetServlet1</servlet-name>
    <url-pattern>/onlyant/greet1</url-pattern>
  </servlet-mapping>
  
  
